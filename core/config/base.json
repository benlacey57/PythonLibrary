{
  "app": {
    "name": "Python Utility Library",
    "version": "0.1.0"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "services": {
    "redis": {
      "enabled": false,
      "host": "localhost",
      "port": 6379
    },
    "database": {
      "enabled": false,
      "type": "postgresql",
      "host": "localhost",
      "port": 5432,
      "name": "app_db"
    }
  }
}